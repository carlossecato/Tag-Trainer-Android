<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tagandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.tagandroid.ui.activity.CartActivity"></activity>
        <activity android:name="com.example.tagandroid.ui.activity.PurchaseActivity" />
        <activity android:name="com.example.tagandroid.ui.activity.CheckoutActivity" />
        <activity android:name="com.example.tagandroid.ui.activity.ProductsActivity" />
        <activity android:name="com.example.tagandroid.ui.activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>